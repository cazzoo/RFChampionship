{% extends "RFCCoreBundle::layout.html.twig" %} {% block title %} Admin
- {{ parent() }} {% endblock %} {% block rfccore_body %}
<h1>Track</h1>

<table class="record_properties">
	<tbody>
		<tr>
			<th>Id</th>
			<td>{{ entity.id }}</td>
		</tr>
		<tr>
			<th>Name</th>
			<td>{{ entity.name }}</td>
		</tr>
	</tbody>
</table>

<ul class="record_actions">
	<li><a href="{{ path('admin_track', {'gameId' : gameId}) }}"> Back to
			the list </a></li>
	<li><a
		href="{{ path('admin_track_edit', { 'id': entity.id, 'gameId' : gameId }) }}">
			Edit </a></li>
	<li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
